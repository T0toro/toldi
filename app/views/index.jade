extends layout

block content
  .home-page-wrapper#home-page
    form(method="GET", action="/search", id="main-page-search")
      .form-group
        input(type="text", name="word", class="form-control form-input-search")
        button(class="btn btn-search btn-icon ion-ios-search", name="submit", id="btn-form-submit")
    section.list.list-word
